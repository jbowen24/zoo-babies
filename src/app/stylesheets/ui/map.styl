.map{
  position absolute;
  height: 100%;
  width: 100%;
  top: 0;
  background-color: white;

  &.active{
    z-index: 100;
  }
}

.multiTip{
  min-width: 60px;
  max-width: 200px;
  border:2px #444 solid;
  overflow:hidden;
  word-wrap: break-word;
  font-size:12px;
  text-align:center;
  border-radius:5px;
  -moz-box-shadow: 2px 2px 10px #333;
  -webkit-box-shadow: 2px 2px 10px #333;
  box-shadow: 2px 2px 10px #333;
}